{
  "name": "web",
  "version": "0.0.1",
  "description": "priority list",
  "main": "index.js",
  "repository": "git@github.com:vianch/priorityList.git",
  "author": "VIANCH",
  "license": "MIT",
  "scripts": {
    "commit": "cz --version",
    "changelog": "conventional-changelog -p react -i CHANGELOG.md -s && git add CHANGELOG.md",
    "dev": "cross-env APP_ENV=local next dev",
    "build": "cross-env APP_ENV=production next build",
    "export": "cross-env APP_ENV=production npm run build && next export -o _static",
    "init": "rm -rf package-lock.json && rm -rf yarn.lock && rm -rf yarn-error.log && rm -rf node_modules &&  rm -rf .next &&  rm -rf _static && yarn install",
    "lint:staged": "lint-staged",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:init": "next lint",
    "lint:fix": "npm run lint -- --fix",
    "pub:patch": "npm --no-git-tag-version version patch",
    "pub:minor": "npm --no-git-tag-version version minor",
    "pub:major": "npm --no-git-tag-version version major",
    "pub:prerelease": "npm version prerelease --preid=alpha",
    "prepare": "husky install",
    "start": "cross-env APP_ENV=production next start -H 0.0.0.0 -p ${PORT:-3000}",
    "default-start": "cross-env APP_ENV=production next start",
    "test": "JEST_JUNIT_OUTPUT=reports/junit/junit_client.xml jest --ci --coverageDirectory=reports/coverage/client --coverage --maxWorkers 2 --logHeapUsage"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "lodash": "^4.17.21",
    "next": "^11.1.3",
    "next-redux-wrapper": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.7.1",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.2",
    "@types/react": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
